LL   = -lpthread -I/usr/include/mysql -L/usr/lib64/mysql -lmysqlclient_r 
OBJPB = probe.o
OBJ  = coverge.o
COMP = compare.o
OBJM = main.o
all  : roucoverge compare probe
roucoverge : $(OBJ) $(OBJM)
	cc -o $@  $(LL) $(OBJ) $(OBJM)
$(OBJ) : coverge.c coverge.h
	cc -c -g -Wall -Werror   coverge.c  $(LL)
$(OBJM) : main.c  coverge.h
	cc -c -g -Wall -Werror  main.c   $(LL)
	
compare : $(COMP)
	cc -o compare $(COMP) 
$(COMP) : compare.c  coverge.h
	cc -c -g -Wall -Werror  compare.c 
probe :   $(OBJPB)
	cc -o probe $(OBJPB)
$(OBJPB) : probe.c coverge.h
	cc -c -g -Wall -Werror  probe.c
.PHONY: clean
clean :
	rm -f roucoverge 
	rm -f compare 
	rm -f probe
	rm -f *.o
	